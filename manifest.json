{

"manifest_version": 2,
"name": "Noteefy",
"version": "1.0",
"content_scripts":[
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
],
"background":{
    "scripts" : ["background.js"]
},

"browser_action":
{
    "default_icon": "/img/icon128.png",
    "icons":
    {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "Noteefy"
}
}